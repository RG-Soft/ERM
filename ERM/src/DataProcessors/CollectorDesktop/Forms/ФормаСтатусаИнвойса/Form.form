      <textEdit>true</textEdit>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>1) Customer is insolvent or bankrupt/Bad Debt/Litigation</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.CustomerIsInsolventOrBankrupt</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>2) Dispute: Pending SLB Documentation</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.PendingSLBDocumentation</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>3) Dispute: Pricing</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.Pricing</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>4) Dispute: Technical/SQ </value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.TechnicalSQ</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>5) Dispute: Lost in hole (LiH) / DBR</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.LostInHole</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>6) Pending invoice approval (no dispute)</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.PendingInvoiceApproval</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>7) Pending payment allocation</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.PendingPaymentAllocation</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>8) Client Confirmed Payment</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.ClientConfirmedPayment</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>9) Invoice Paid</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.InvoicePaid</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>10) Escalated to Sales/Account Mngr</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.EscalatedToSales</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>12) Customer is unavailable, repeat call</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.CustomerIsUnavailable</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>13) Client has liquidity problems</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.ClientHasLiquidityProblems</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>14) Billing error</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.BillingError</value>
        </value>
      </choiceList>
      <choiceList>
        <presentation>
          <key>ru</key>
          <value>15) Partially paid</value>
        </presentation>
        <value xsi:type="core:ReferenceValue">
          <value>Enum.InvoiceStatus.EnumValue.PartiallyPaid</value>
        </value>
      </choiceList>
    </extInfo>
