	ВложенныйЗапрос.Agreement,
	ВложенныйЗапрос.CustomerID,
	ВложенныйЗапрос.Aging,
	ВложенныйЗапрос.Rate,
	ВложенныйЗапрос.LegalEntity,
	ВложенныйЗапрос.DateComment,
	ВЫРАЗИТЬ(ВложенныйЗапрос.ActionItem КАК СТРОКА(1024)),
	ВЫРАЗИТЬ(ВложенныйЗапрос.ProblemDetails КАК СТРОКА(1024)),
	ВложенныйЗапрос.WellData

ИМЕЮЩИЕ
	СУММА(ВложенныйЗапрос.RemainingAmount) &lt;> 0</queryText>
