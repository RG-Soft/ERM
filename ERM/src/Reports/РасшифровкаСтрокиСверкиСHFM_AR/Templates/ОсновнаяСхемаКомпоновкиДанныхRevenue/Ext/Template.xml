	И RevenueОбороты.AU.ПодразделениеОрганизации.БазовыйЭлемент = &amp;ЛокацияHFM
	И RevenueОбороты.Account.БазовыйЭлемент В ИЕРАРХИИ(&amp;AccountHFM)</query>
