			И ВсеПериодыПоКомбинацииАналитики.LegalEntity = ВТ_Биллинг.LegalEntity
			И ВсеПериодыПоКомбинацииАналитики.AU = ВТ_Биллинг.AU
			И ВсеПериодыПоКомбинацииАналитики.Source = ВТ_Биллинг.Source

