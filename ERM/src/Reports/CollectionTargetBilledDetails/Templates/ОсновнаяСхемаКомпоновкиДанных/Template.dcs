	ЕСТЬNULL(InvoiceCommentsСрезПоследних.Problem.RWDTargetDate, ДАТАВРЕМЯ(1, 1, 1)) КАК RWDTargetDate,
	ЕСТЬNULL(InvoiceCommentsСрезПоследних.Problem.Potential, "") КАК Potential,
