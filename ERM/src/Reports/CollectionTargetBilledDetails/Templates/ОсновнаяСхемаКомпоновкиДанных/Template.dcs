	ВТ_РассчитанныеРесурсыУровень6.InvoiceAmount КАК InvoiceAmount,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceContractTrigger КАК InvoiceContractTrigger,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceTriggerDate КАК InvoiceTriggerDate,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceContractPTDaysFrom КАК InvoiceContractPTDaysFrom,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceContractСрокОплаты КАК InvoiceContractСрокОплаты,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceDueDateFrom КАК InvoiceDueDateFrom,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceDueDateTo КАК InvoiceDueDateTo,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceStatusOfDispute КАК InvoiceStatusOfDispute,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceDisputeDistributedDate КАК InvoiceDisputeDistributedDate,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceDisputeCollectableDate КАК InvoiceDisputeCollectableDate,
	ВТ_РассчитанныеРесурсыУровень6.DateEntered КАК DateEntered,
	ВТ_РассчитанныеРесурсыУровень6.DateIdentified КАК DateIdentified,
	ВТ_РассчитанныеРесурсыУровень6.InvoiceContractAmendmentName КАК InvoiceContractAmendmentName,
	ВТ_РассчитанныеРесурсыУровень6.TransType КАК TransType,
	ВТ_РассчитанныеРесурсыУровень6.Paid КАК Paid,
	ВТ_РассчитанныеРесурсыУровень6.PaymentDate КАК PaymentDate,
	ВТ_РассчитанныеРесурсыУровень6.BrokenPromises КАК BrokenPromises,
	ВТ_РассчитанныеРесурсыУровень6.EscalationSent КАК EscalationSent,
	ВТ_РассчитанныеРесурсыУровень6.LegalEntity КАК LegalEntity,
	ВТ_РассчитанныеРесурсыУровень6.USDAmount КАК USDAmount,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.DueFrom КАК ЧИСЛО(15, 2)) КАК DueFrom,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.DueTo КАК ЧИСЛО(15, 2)) КАК DueTo,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.UnderLitigation КАК ЧИСЛО(15, 2)) КАК UnderLitigation,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.NoFrcst КАК ЧИСЛО(15, 2)) КАК NoFrcst,
	ВЫБОР
		КОГДА &amp;ОбнулятьForecastCollectBefore10 = ИСТИНА
		И ВТ_РассчитанныеРесурсыУровень6.Status = ЗНАЧЕНИЕ(Перечисление.InvoiceStatus.InvoicePaid)
		И
		НЕ ВТ_РассчитанныеРесурсыУровень6.Paid = 0
		И ВТ_РассчитанныеРесурсыУровень6.FcstCollect МЕЖДУ -10 И 10
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.FcstCollect КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК FcstCollect,
	ВТ_РассчитанныеРесурсыУровень6.OverdueDispute КАК OverdueDispute,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.OverdueNoFcst КАК ЧИСЛО(15, 2)) КАК OverdueNoFcst,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.InRangeNoColl КАК ЧИСЛО(15, 2)) КАК InRangeNoColl,
	ВЫРАЗИТЬ(ВТ_РассчитанныеРесурсыУровень6.NotDue КАК ЧИСЛО(15, 2)) КАК NotDue,
	ВТ_РассчитанныеРесурсыУровень6.Collector КАК Collector,
	ВТ_РассчитанныеРесурсыУровень6.Invoice КАК Invoice,
	ВТ_РассчитанныеРесурсыУровень6.LocationHFM КАК LocationHFM,
	ВТ_РассчитанныеРесурсыУровень6.SegmentHFM КАК SegmentHFM,
	ВТ_РассчитанныеРесурсыУровень6.MgmtGeomarket КАК MgmtGeomarket,
	ВТ_РассчитанныеРесурсыУровень6.SubSegment КАК SubSegment,
	ВТ_РассчитанныеРесурсыУровень6.GeoMarket КАК GeoMarket,
	ВТ_РассчитанныеРесурсыУровень6.CompanyHFM КАК CompanyHFM,
	ВЫБОР
		КОГДА ВТ_РассчитанныеРесурсыУровень6.Potential = ЗНАЧЕНИЕ(Перечисление.InvoicePotential.PotentialForecast)
			ТОГДА ВТ_РассчитанныеРесурсыУровень6.InRangeNoColl + ВТ_РассчитанныеРесурсыУровень6.OverdueDispute +
				ВТ_РассчитанныеРесурсыУровень6.OverdueNoFcst + ВТ_РассчитанныеРесурсыУровень6.NotDue +
				ВТ_РассчитанныеРесурсыУровень6.NoFrcst + ВТ_РассчитанныеРесурсыУровень6.UnderLitigation
		ИНАЧЕ 0
	КОНЕЦ КАК PotentialForecast,
	ВЫБОР
		КОГДА ВТ_РассчитанныеРесурсыУровень6.Potential = ЗНАЧЕНИЕ(Перечисление.InvoicePotential.PotentialRisk)
			ТОГДА -ВТ_РассчитанныеРесурсыУровень6.FcstCollect
		ИНАЧЕ 0
	КОНЕЦ КАК PotentialRisk
ИЗ
	ВТ_РассчитанныеРесурсыУровень6 КАК ВТ_РассчитанныеРесурсыУровень6</query>
