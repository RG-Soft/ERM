		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	SalesOrder.Source КАК Source,
	SalesOrder.Location,
	SalesOrder.Company.Код,
	SalesOrder.Company.Наименование,
	SalesOrder.Client КАК Client,
	SalesOrder.Account,
	SalesOrder.AU,
	SalesOrder.Invoice,
	SalesOrder.Currency,
	SalesOrder.Amount,
	SalesOrder.Agreement,
	SalesOrder.FieldTicket,
	SalesOrder.SiebelOrderId,
	SalesOrder.JobEndDate,
	SalesOrder.FTLSubmissionDate,
	SalesOrder.CreationDate,
	SalesOrder.ApprovalDate,
	SalesOrder.FirstSubmissionDate,
	SalesOrder.InvoiceFlagDate,
	SalesOrder.ERPStatus,
	SalesOrder.FTLCreatedBy,
	SalesOrder.FTLApproverID,
	SalesOrder.ApprovedBy,
	SalesOrder.CreatedBy,
	SalesOrder.Номер,
	SalesOrder.InvoiceType,
	SalesOrder.OrderType,
	SalesOrder.Ссылка,
	SalesOrder.Company,
	SalesOrder.SubSubSegment,
	ВЫБОР
		КОГДА SalesOrder.Currency = &amp;USD
			ТОГДА SalesOrder.Amount
		КОГДА ВнутренниеКурсыВалютСрезПоследних.Курс ЕСТЬ NULL 
				ИЛИ ВнутренниеКурсыВалютСрезПоследних.Курс = 0
			ТОГДА 0
		ИНАЧЕ SalesOrder.Amount / ВнутренниеКурсыВалютСрезПоследних.Курс
	КОНЕЦ КАК BaseAmount
ПОМЕСТИТЬ ВТ_SO
ИЗ
	Документ.SalesOrder КАК SalesOrder
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ВнутренниеКурсыВалют.СрезПоследних({(&amp;КонецПериода)}, ) КАК ВнутренниеКурсыВалютСрезПоследних
		ПО SalesOrder.Currency = ВнутренниеКурсыВалютСрезПоследних.Валюта
ГДЕ
	НЕ SalesOrder.ПометкаУдаления
	И SalesOrder.InvoiceFlagDate &gt;= &amp;НачалоПериода
	И SalesOrder.InvoiceFlagDate &lt;= &amp;КонецПериода

ИНДЕКСИРОВАТЬ ПО
	Client,
	Source
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипСоответствия КАК ТипСоответствия,
	МАКСИМУМ(НастройкаСинхронизацииОбъектовСВнешнимиСистемами.Период) КАК Период,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ОбъектПриемника
ПОМЕСТИТЬ ВТ_ИдентификаторыКлиентов
ИЗ
	ВТ_SO КАК ВТ_SO
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкаСинхронизацииОбъектовСВнешнимиСистемами КАК НастройкаСинхронизацииОбъектовСВнешнимиСистемами
		ПО ВТ_SO.Source = НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипСоответствия
			И ВТ_SO.Client = НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ОбъектПриемника
ГДЕ
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипОбъектаВнешнейСистемы = &amp;ТипОбъектаВнешнейСистемы

СГРУППИРОВАТЬ ПО
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипСоответствия,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ОбъектПриемника

ИНДЕКСИРОВАТЬ ПО
	ТипСоответствия
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипСоответствия КАК ТипСоответствия,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.Идентификатор,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ОбъектПриемника КАК Client
ПОМЕСТИТЬ ВТ_ИдентификаторыКлиентовИтог
ИЗ
	РегистрСведений.НастройкаСинхронизацииОбъектовСВнешнимиСистемами КАК НастройкаСинхронизацииОбъектовСВнешнимиСистемами
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ИдентификаторыКлиентов КАК ВТ_ИдентификаторыКлиентов
		ПО НастройкаСинхронизацииОбъектовСВнешнимиСистемами.Период = ВТ_ИдентификаторыКлиентов.Период
			И НастройкаСинхронизацииОбъектовСВнешнимиСистемами.ТипСоответствия = ВТ_ИдентификаторыКлиентов.ТипСоответствия

ИНДЕКСИРОВАТЬ ПО
	ТипСоответствия,
	Client
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	UnbilledARОстатки.Client КАК Client,
	UnbilledARОстатки.Company КАК Company,
	UnbilledARОстатки.Source КАК Source,
	UnbilledARОстатки.Location КАК Location,
	UnbilledARОстатки.SubSubSegment КАК SubSubSegment,
	UnbilledARОстатки.SalesOrder КАК SalesOrder,
	UnbilledARОстатки.AmountОстаток,
	UnbilledARОстатки.BaseAmountОстаток
ПОМЕСТИТЬ ВТ_Остатки
ИЗ
	РегистрНакопления.UnbilledAR.Остатки(
			{(&amp;КонецПериода)},
			(Client, Company, Source, Location, SubSubSegment, SalesOrder) В
				(ВЫБРАТЬ
					ВТ_SO.Client,
					ВТ_SO.Company,
					ВТ_SO.Source,
					ВТ_SO.Location,
					ВТ_SO.SubSubSegment КАК Segment2,
					ВТ_SO.Ссылка КАК Source1
				ИЗ
					ВТ_SO КАК ВТ_SO)) КАК UnbilledARОстатки

ИНДЕКСИРОВАТЬ ПО
	Client,
	Company,
	Source,
	Location,
	SubSubSegment,
	SalesOrder
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_SO.Source,
	ВТ_SO.Location,
	ВТ_SO.CompanyКод,
	ВТ_SO.CompanyНаименование,
	ВТ_SO.Client,
	ВТ_SO.Account,
	ВТ_SO.AU,
	ВТ_SO.Invoice,
	ВТ_SO.Currency,
	ВТ_SO.Amount,
	ВТ_SO.Agreement,
	ВТ_SO.FieldTicket,
	ВТ_SO.SiebelOrderId,
	ВТ_SO.JobEndDate,
	ВТ_SO.FTLSubmissionDate,
	ВТ_SO.CreationDate,
	ВТ_SO.ApprovalDate,
	ВТ_SO.FirstSubmissionDate,
	ВТ_SO.InvoiceFlagDate,
	ВТ_SO.ERPStatus,
	ВТ_SO.FTLCreatedBy,
	ВТ_SO.FTLApproverID,
	ВТ_SO.ApprovedBy,
	ВТ_SO.CreatedBy,
	ВТ_SO.Номер,
	ВТ_SO.InvoiceType,
	ВТ_SO.OrderType,
	ВТ_ИдентификаторыКлиентовИтог.Идентификатор КАК CustomerID,
	РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, СЕКУНДА) / 86400 КАК Stage1,
	РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, СЕКУНДА) / 86400 КАК Stage2,
	РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, СЕКУНДА) / 86400 КАК Stage3,
	РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, СЕКУНДА) / 86400 КАК Stage4,
	РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, СЕКУНДА) / 86400 КАК Stage5,
	ВЫРАЗИТЬ(РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, СЕКУНДА) / 86400 КАК ЧИСЛО(10, 0)) КАК TotalDIR,
	(ВЫРАЗИТЬ(ВТ_SO.BaseAmount КАК ЧИСЛО(10, 2))) * (ВЫРАЗИТЬ(РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, СЕКУНДА) / 86400 + РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, СЕКУНДА) / 86400 КАК ЧИСЛО(10, 0))) КАК DirFactor,
	ВЫБОР
		КОГДА ВТ_Остатки.AmountОстаток ЕСТЬ NULL 
			ТОГДА "Billed"
		ИНАЧЕ "Unbilled"
	КОНЕЦ КАК BiilledType,
	ВТ_SO.SubSubSegment,
	ВЫРАЗИТЬ(ВТ_SO.BaseAmount КАК ЧИСЛО(10, 2)) КАК BaseAmount
ИЗ
	ВТ_SO КАК ВТ_SO
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Остатки КАК ВТ_Остатки
		ПО ВТ_SO.Client = ВТ_Остатки.Client
			И ВТ_SO.Company = ВТ_Остатки.Company
			И ВТ_SO.Source = ВТ_Остатки.Source
			И ВТ_SO.Location = ВТ_Остатки.Location
			И ВТ_SO.SubSubSegment = ВТ_Остатки.SubSubSegment
			И ВТ_SO.Ссылка = ВТ_Остатки.SalesOrder
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ИдентификаторыКлиентовИтог КАК ВТ_ИдентификаторыКлиентовИтог
		ПО ВТ_SO.Source = ВТ_ИдентификаторыКлиентовИтог.ТипСоответствия
			И ВТ_SO.Client = ВТ_ИдентификаторыКлиентовИтог.Client</query>
	</dataSet>
	<parameter>
		<name>КонецПериода</name>
		<title xsi:type="v8:LocalStringType">
