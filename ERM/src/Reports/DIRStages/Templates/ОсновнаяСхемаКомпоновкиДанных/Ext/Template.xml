		<query>ВЫБРАТЬ
	SalesOrder.Source КАК Source,
	SalesOrder.GeoMarket,
	SalesOrder.SubGeoMarket,
	SalesOrder.Segment,
	SalesOrder.Company.Код,
	SalesOrder.Company.Наименование,
	SalesOrder.Client КАК Client,
	SalesOrder.Account,
	SalesOrder.AU,
	SalesOrder.Invoice,
	SalesOrder.Currency,
	SalesOrder.Amount,
	SalesOrder.Agreement,
	SalesOrder.FieldTicket,
	SalesOrder.SiebelOrderId,
	SalesOrder.JobEndDate,
	SalesOrder.FTLSubmissionDate,
	SalesOrder.CreationDate,
	SalesOrder.ApprovalDate,
	SalesOrder.FirstSubmissionDate,
	SalesOrder.InvoiceFlagDate,
	SalesOrder.ERPStatus,
	SalesOrder.FTLCreatedBy,
	SalesOrder.FTLApproverID,
	SalesOrder.ApprovedBy,
	SalesOrder.CreatedBy,
	SalesOrder.Номер,
	SalesOrder.InvoiceType,
	SalesOrder.OrderType,
	SalesOrder.BaseAmount
ПОМЕСТИТЬ ВТ_SO
ИЗ
	Документ.SalesOrder КАК SalesOrder
ГДЕ
	НЕ SalesOrder.ПометкаУдаления
	И SalesOrder.InvoiceFlagDate &gt;= &amp;НачалоПериода
	И SalesOrder.InvoiceFlagDate &lt;= &amp;КонецПериода

ИНДЕКСИРОВАТЬ ПО
	Client,
	Source
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	НастройкаСинхронизацииОбъектовСВнешнимиСистемамиСрезПоследних.ТипСоответствия КАК ТипСоответствия,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемамиСрезПоследних.Идентификатор,
	НастройкаСинхронизацииОбъектовСВнешнимиСистемамиСрезПоследних.ОбъектПриемника КАК Client
ПОМЕСТИТЬ ВТ_ИдентификаторыКлиентов
ИЗ
	РегистрСведений.НастройкаСинхронизацииОбъектовСВнешнимиСистемами.СрезПоследних(
			{(&amp;КонецПериода)},
			ТипОбъектаВнешнейСистемы = ЗНАЧЕНИЕ(Перечисление.ТипыОбъектовВнешнихСистем.Client)
				И (ТипСоответствия, ОбъектПриемника) В
					(ВЫБРАТЬ РАЗЛИЧНЫЕ
						ВТ_SO.Source,
						ВТ_SO.Client
					ИЗ
						ВТ_SO КАК ВТ_SO)) КАК НастройкаСинхронизацииОбъектовСВнешнимиСистемамиСрезПоследних

ИНДЕКСИРОВАТЬ ПО
	ТипСоответствия,
	Client
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_SO.Source,
	ВТ_SO.GeoMarket,
	ВТ_SO.SubGeoMarket,
	ВТ_SO.Segment,
	ВТ_SO.CompanyКод,
	ВТ_SO.CompanyНаименование,
	ВТ_SO.Client,
	ВТ_SO.Account,
	ВТ_SO.AU,
	ВТ_SO.Invoice,
	ВТ_SO.Currency,
	ВТ_SO.Amount,
	ВТ_SO.Agreement,
	ВТ_SO.FieldTicket,
	ВТ_SO.SiebelOrderId,
	ВТ_SO.JobEndDate,
	ВТ_SO.FTLSubmissionDate,
	ВТ_SO.CreationDate,
	ВТ_SO.ApprovalDate,
	ВТ_SO.FirstSubmissionDate,
	ВТ_SO.InvoiceFlagDate,
	ВТ_SO.ERPStatus,
	ВТ_SO.FTLCreatedBy,
	ВТ_SO.FTLApproverID,
	ВТ_SO.ApprovedBy,
	ВТ_SO.CreatedBy,
	ВТ_SO.Номер,
	ВТ_SO.InvoiceType,
	ВТ_SO.OrderType,
	ВТ_ИдентификаторыКлиентов.Идентификатор КАК CustomerID,
	РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, ДЕНЬ) КАК Stage1,
	РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, ДЕНЬ) КАК Stage2,
	РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, ДЕНЬ) КАК Stage3,
	РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, ДЕНЬ) КАК Stage4,
	РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, ДЕНЬ) КАК Stage5,
	РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, ДЕНЬ) КАК TotalDIR,
	ВТ_SO.BaseAmount * (РАЗНОСТЬДАТ(ВТ_SO.JobEndDate, ВТ_SO.FTLSubmissionDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.FTLSubmissionDate, ВТ_SO.CreationDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.CreationDate, ВТ_SO.ApprovalDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.ApprovalDate, ВТ_SO.FirstSubmissionDate, ДЕНЬ) + РАЗНОСТЬДАТ(ВТ_SO.FirstSubmissionDate, ВТ_SO.InvoiceFlagDate, ДЕНЬ)) КАК DirFactor
ИЗ
	ВТ_SO КАК ВТ_SO
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ИдентификаторыКлиентов КАК ВТ_ИдентификаторыКлиентов
		ПО ВТ_SO.Source = ВТ_ИдентификаторыКлиентов.ТипСоответствия
			И ВТ_SO.Client = ВТ_ИдентификаторыКлиентов.Client</query>
